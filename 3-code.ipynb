    "S = set({0,1})\n",
    "\n",
    "subsets = list(product(S, repeat=9))\n",
    "count = 0\n",
    "for item in subsets:\n",
    "    cc = 0\n",
    "    for i in item:\n",
    "        if i == 1:\n",
    "            cc += 1\n",
    "    if cc >=5:\n",
    "        count+=1\n",
    "print(count)\n"
